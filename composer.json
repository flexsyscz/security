{
	"name": "flexsyscz/security",
	"description": "Security package for better support of PHP (Nette) web applications.",
	"keywords": ["flexsyscz", "security", "firewall"],
	"homepage": "https://www.flexsys.cz",
	"license": ["BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"authors": [
		{
			"name": "Václav Lukeš",
			"homepage": "https://www.vaclavlukes.cz"
		}
	],
	"require": {
		"php": ">=8.1",
		"tracy/tracy": "^2.9.5",
		"flexsyscz/environment": "^1.0",
		"nette/utils": "^4.0",
		"nette/security": "^3.1",
		"nette/robot-loader": "^4.0"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan-nette": "^1.2",
		"nette/coding-standard": "^3.3"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"minimum-stability": "stable",
	"scripts": {
		"phpstan": "phpstan analyse",
		"tester": "tester tests -s"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
